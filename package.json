{
  "name": "cook-bisheng",
  "version": "0.29.09",
  "description": "Transform Markdown(and other static files with transformers) into a SPA website using React.",
  "bin": {
    "bisheng": "./bin/bisheng"
  },
  "files": [
    "lib",
    "bin",
    "router.js",
    "collect.js"
  ],
  "scripts": {
    "build": "babel src --out-dir lib",
    "clean": "rimraf lib",
    "pubbeta": "npm publish --tag beta",
    "pub": "node ./publish.js"
  },
  "babel": {
    "presets": [
      "@babel/preset-react",
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "6"
          }
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-object-rest-spread"
    ]
  },
  "keywords": [
    "markdown",
    "spa",
    "website",
    "blog",
    "react"
  ],
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^7.1.4",
    "babel-jest": "^24.5.0",
    "babel-loader": "8",
    "babel-plugin-import": "^1.11.0",
    "case-sensitive-paths-webpack-plugin": "^2.0.0",
    "chalk": "^1.1.3",
    "commander": "^2.9.0",
    "cook-mark-twain": "2",
    "css-loader": "~0.28.0",
    "exist.js": "^0.3.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "gh-pages": "^1.0.0",
    "history": "^3.0.0",
    "jsonml-to-react-element": "^1.0.0",
    "jsonml.js": "^0.1.0",
    "less": "^2.7.3",
    "less-loader": "~4.0.5",
    "loader-utils": "^1.1.0",
    "mkdirp": "^0.5.1",
    "node-prismjs": "^0.1.0",
    "nprogress": "^0.2.0",
    "nunjucks": "^2.5.2",
    "postcss": "^7.0.14",
    "postcss-loader": "^3.0.0",
    "prismjs": "^1.4.1",
    "ramda": "^0.22.0",
    "react": "^16.0.0",
    "react-dev-utils": "^4.1.0",
    "react-dom": "^16.0.0",
    "react-router": "^3.0.0",
    "resolve": "^1.1.7",
    "rimraf": "^2.5.4",
    "rucksack-css": "~1.0.2",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.1",
    "ts-jest": "24",
    "ts-loader": "2",
    "uglifyjs-webpack-plugin": "^1.1.2",
    "url-loader": "~0.5.8",
    "webpack": "^3.7.0",
    "webpack-dev-server": "^2.9.1",
    "webpackbar": "^3.1.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "shx": "^0.3.2"
  }
}
